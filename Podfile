# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'iTunesSearchSample' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for iTunesSearchSample
pod 'Alamofire', '~> 4.7'
pod 'AlamofireImage', '~> 3.5'
pod 'RealmSwift'


  target 'iTunesSearchSampleTests' do
    inherit! :search_paths
    # Pods for testing
pod 'Alamofire', '~> 4.7'
pod 'AlamofireImage', '~> 3.5'
pod 'RealmSwift'


  end

  target 'iTunesSearchSampleUITests' do
    inherit! :search_paths
    # Pods for testing
pod 'Alamofire', '~> 4.7'
pod 'AlamofireImage', '~> 3.5'
pod 'RealmSwift'
  end

end
